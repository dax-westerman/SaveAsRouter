{
  "manifest_version": 3,
  "name": "SaveAsRouter - Offline Menu",
  "version": "1.0.0",
  "description": "Chrome extension that adds an 'Offline' context menu with configurable sub-menu items",
  "permissions": [
    "contextMenus",
    "storage",
    "scripting",
    "activeTab"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dist/content.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}